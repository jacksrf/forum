<%= form_for @entry do |f| %>
 <%= f.label :title %>
 <%= f.text_field :title %>
 </br >
 <%= f.label :content %>
 <%= f.text_area :content %>
 </br >
 <%= f.hidden_field :user_id, :value => session[:user_id] %>
 </br >
 <%= f.submit %>
<% end %>
<ul>
  <% @entry.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
  <% end %>
</ul>
